package org.simol.examples.design_patterns.composite.improvements;

// Leaf class
simu_real_world_obj File contracts FileSystemComponent {
    private String name;

    public File(String name) {
        this.name = name;
    }

    public void done(){
        this.display();
    }

    //Realization on computer stuff
    private void display() {
        System.out.println("File: " + name);
    }
}
